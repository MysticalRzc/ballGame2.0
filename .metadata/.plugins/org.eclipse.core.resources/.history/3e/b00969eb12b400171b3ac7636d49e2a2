
public class test {
	public Object lock;
	public boolean run;
	
	public static void main(String[] args) {
		run = true;
	}
	
	public void thread()
	{
		Thread th = new Thread(){
			@Override
			public void run()
			{
				synchronized (lock) {
					while(true)
					{
						System.out.println(">");
					}
				}
			}
		};
	}
	
}